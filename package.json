{
  "name": "react_system",
  "version": "1.0.0",
  "description": "react+ts+react-router+antd+webpack+redux",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development two start",
    "build": "rimraf dist && cross-env NODE_ENV=production two build",
    "eslint:test": "eslint src --ext .js,.ts,.tsx",
    "stylelint:test": "stylelint src --fix .less,.css",
    "lint:fix": "eslint src --fix --ext .js,.ts,.tsx",
    "prettier": "prettier --write src/*",
    "test": "BABEL_ENV=test jest",
    "test:watch": "BABEL_ENV=test jest --watch",
    "test:coverage": "BABEL_ENV=test jest --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "sideEffects": [
    "dist/*",
    "mock/*",
    "*.less"
  ],
  "repository": "git+https://github.com/qiuysh/react_system.git",
  "author": "wulin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qiuysh/react_system/issues"
  },
  "homepage": "https://github.com/qiuysh/react_system#readme",
  "dependencies": {
    "@ant-design/charts": "^1.1.2",
    "@ant-design/icons": "^4.4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "antd": "^4.11.2",
    "axios": ">=0.18.1",
    "classnames": "^2.2.5",
    "crypto": "^1.0.1",
    "history": "^4.10.1",
    "immutability-helper-x": "^1.0.5",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.2",
    "prop-types": "^15.7.2",
    "qs": "^6.9.6",
    "react": "^16.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.11.0",
    "react-redux": "^7.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.20",
    "@types/react": "^16.14.2",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.9.10",
    "@types/react-router-dom": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "body-parser": "^1.15.2",
    "create-two-scripts": "^1.0.8-alpha.0",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-wolin": "git+ssh://git@github.com:qiuysh/eslint-config-wolin.git",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "lint-staged": "^10.5.3",
    "mocker-api": "^2.9.4",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "redbox-react": "^1.3.3",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.5.4",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^26.5.5",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{less,css}": [
      "stylelint --fix"
    ],
    "src/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
